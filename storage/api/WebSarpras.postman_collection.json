{
  "info": {
    "_postman_id": "web-sarpras-api-collection",
    "name": "Web Sarpras API",
    "description": "API documentation for Web Sarpras application",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "item": [
    {
      "name": "Authentication",
      "item": [
        {
          "name": "Get User Info",
          "request": {
            "method": "GET",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{bearer_token}}",
                "type": "text"
              },
              {
                "key": "Accept",
                "value": "application/json",
                "type": "text"
              }
            ],
            "url": {
              "raw": "{{base_url}}/api/user",
              "host": ["{{base_url}}"],
              "path": ["api", "user"]
            }
          },
          "response": []
        }
      ]
    },
    {
      "name": "Borrowing",
      "item": [
        {
          "name": "Check Asset Availability",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Authorization",
                "value": "Bearer {{bearer_token}}",
                "type": "text"
              },
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              },
              {
                "key": "Accept",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"tanggal_mulai\": \"2023-12-01\",\n  \"tanggal_selesai\": \"2023-12-05\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/check-availability/{{asset_id}}",
              "host": ["{{base_url}}"],
              "path": ["api", "check-availability", "{{asset_id}}"]
            }
          },
          "response": []
        }
      ]
    }
  ],
  "event": [
    {
      "listen": "prerequest",
      "script": {
        "type": "text/javascript",
        "exec": [""]
      }
    },
    {
      "listen": "test",
      "script": {
        "type": "text/javascript",
        "exec": [""]
      }
    }
  ],
  "variable": [
    {
      "key": "base_url",
      "value": "http://localhost:8000",
      "type": "string"
    },
    {
      "key": "asset_id",
      "value": "1",
      "type": "string"
    }
  ]
}